{
  "name": "api-despesas",
  "version": "1.0.0",
  "description": "Aplicação voltada para gerenciamento de despesas",
  "author": "João Pedro <joaopedromoraesnsilva@gmail.com>",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky install",
    "migrate:generate": "prisma migrate dev && prisma generate",
    "migrate:execute": "prisma migrate deploy",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "start:dev": "yarn migrate:execute && nest start",
    "start:dev:watch": "yarn migrate:execute && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start": "yarn migrate:execute && yarn build && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --passWithNoTests --detectOpenHandles --silent --forceExit",
    "test:coverage": "yarn test --coverage",
    "test:staged": "yarn test --findRelatedTests",
    "test:watch": "yarn test --watchAll",
    "test:unit": "yarn test -c jest-unit-config.js",
    "test:integration": "yarn test -c jest-integration-config.js",
    "test:unit:watch": "yarn test --watchAll -c jest-unit-config.js",
    "test:integration:watch": "yarn test --watchAll -c jest-integration-config.js",
    "test:verbose": "jest --passWithNoTests --detectOpenHandles --forceExit",
    "test:watch:verbose": "yarn test:verbose --watchAll",
    "test:unit:verbose": "yarn test:verbose -c jest-unit-config.js",
    "test:integration:verbose": "yarn test:verbose -c jest-integration-config.js",
    "test:unit:watch:verbose": "yarn test:verbose --watchAll -c jest-unit-config.js",
    "test:integration:watch:verbose": "yarn test:verbose --watchAll -c jest-integration-config.js"
  },
  "dependencies": {
    "@nestjs/common": "^9.0.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/jwt": "^10.0.3",
    "@nestjs/mongoose": "^9.2.2",
    "@nestjs/passport": "^9.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^6.3.0",
    "@prisma/client": "4.8.1",
    "amqplib": "^0.10.3",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "dotenv": "^16.0.3",
    "express-handlebars": "^7.0.7",
    "mongoose": "^7.0.4",
    "nodemailer": "^6.9.1",
    "nodemailer-express-handlebars": "^6.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "prisma": "4.8.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-conventional": "^17.6.1",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/amqplib": "^0.10.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "29.2.4",
    "@types/node": "18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/nodemailer-express-handlebars": "^4.0.2",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.3",
    "jest": "29.3.1",
    "lint-staged": "^13.2.1",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.7.4"
  }
}
